<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8" />
		<title></title>
	</head>

<nav>
	<a href="/products/new">Add a New Product</a>
</nav>

	<body>
		<h1>Mongoose Store</h1>
		<ul>
			<% products.forEach(product => { %>
            <li>
                <a href="/products/<%=product._id; %>"> <%=product.title; %> </a>
            </li>
			<% }) %>
		</ul>
	</body>
</html>